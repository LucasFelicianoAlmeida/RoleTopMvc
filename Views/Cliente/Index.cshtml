<main>
            <section id="SignUp">

                <h1>Sign In</h1>
                <form method="POST" action='@Url.Action("Login" , "Cliente")'>
                    <div class="default">
                        <div>
                            <label for="email">E-mail</label>
                            <br />
                            <input id="email" type="email"  name="email" required />
                        </div>
                        <div>
                            <label for="senha">Senha</label>
                            <br/>
                            <input id="senha" type="password" name="senha" required/>
                        </div>
                        
                            <input type="submit" value="Entrar!" class="input-style"/>
                        </div>
                        </form>
                        
                        
                </section>
                

    </main>